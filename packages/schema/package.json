{
  "name": "@redux-ai/schema",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm tsc",
    "dev": "pnpm tsc --watch",
    "lint": "pnpm eslint \"src/**/*.{ts,tsx}\"",
    "format": "pnpm prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "pnpm prettier --check \"src/**/*.{ts,tsx,json}\"",
    "test": "pnpm vitest run",
    "test:watch": "pnpm vitest",
    "test:coverage": "pnpm vitest run --coverage",
    "ci": "pnpm run lint && pnpm run format:check && pnpm run test && pnpm run build",
    "preversion": "pnpm run ci"
  },
  "dependencies": {
    "ajv": "^8.17.1"
  },
  "publishConfig": {
    "access": "public"
  }
}